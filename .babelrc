{
  "presets": [
    "react",
    "es2015"
  ],
  "env": {
    "test": {
      "plugins": [
        "istanbul",
        ["babel-root-slash-import", {
          "rootPathSuffix": "src"
        }]
      ]
    },
    "production": {
      "presets": [
        [
          "es2015",
          {
            "modules": false
          }
        ]
      ]
    }
  }
}
